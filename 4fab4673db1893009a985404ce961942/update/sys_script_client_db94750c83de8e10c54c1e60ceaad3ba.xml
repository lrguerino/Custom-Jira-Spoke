        <field>u_jira_ebonding_status</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Hide Jira Section UI</name>
        <order/>
        <script><![CDATA[function onLoad() {
	
    g_form.setSectionDisplay("jira_information", false);

    if (g_form.isNewRecord()) {
        return;
    }

    // If user is not part of the ETS team, we don't show Jira section
    if (!g_user.hasRole("sn_customerservice_agent", true)) {
        return;
    }

    // If server scrachpad not defined, then we don't show Jira section
    if (!g_scratchpad.hasOwnProperty("jiraSettings")) {
        return;
    }

    // We finally know SN and Jira are now bonded, we show the Jira section
    g_form.setSectionDisplay("jira_information", true);

}]]></script>
